		<table align="center">
			<tr>
				<th><label></label></th>
				<th><input type="text" placeholder="Enter Email" name="Email"></th>
			</tr>
			<tr>
				<th><label></label></th>
				<th><input type="password" placeholder="Enter password" name="password"></th>
			</tr>
		</table>
		<div align="center">
			<input type="Submit" class="button" value="Login" id="Login">
		</div>
		</form>
</body>
</html>