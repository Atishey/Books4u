<!DOCTYPE html>
<html>
<head>
	<title>login page</title>
	<style type="text/css">
		img {width: 4cm;
			height: 3cm;}
		.button {background-color: #1a96d6;
				padding: 15px 32px;}
	</style>
</head>

<body>
		<script type="text/javascript">
		function validateForm() {
		    var email = document.forms["myForm"]["Email"].value;
		    var password = document.forms["myForm"]["password"].value;
		    if (email == "" || password == "") {
		        alert("must be filled out");
		            
		        return false;
			}
		}	
		</script>
		<!-- <div class="form"> -->
  		<!-- <div class="thumbnail"><img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/169963/hat.svg"/></div> -->
		<form action="/login" method="post" name="myForm" onsubmit="return validateForm()">
		<img align="center" src="images/logo.jpg" alt="site logo">
		<table align="center">
			<tr>
				<th><label></label></th>
				<th><input type="text" placeholder="Enter Email" name="Email"></th>
			</tr>
			<tr>
				<th><label></label></th>
				<th><input type="password" placeholder="Enter password" name="password"></th>
			</tr>
		</table>
		<div align="center">
			<input type="Submit" class="button" value="Login" id="Login">
		</div>
		</form>
		<!-- </div> -->

		
</body>
</html>